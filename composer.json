{
    "name": "belsrc/php.net",
    "type": "library",
    "description": ".Net like methods and classes in PHP",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/belsrc/PHP-dotNet",
    "keywords": [".net", "collection", "list", "dictionary", "string"],
    "authors": [
        {
            "name": "Bryan Kizer",
            "email": "bryan@bryanckizer.com"
        }
    ],
    "require": {
        "php": ">=5.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.8.*",
        "phpunit/php-code-coverage": "1.3.*",
        "sami/sami": "dev-master",
        "pdepend/pdepend" : "dev-master",
        "sebastian/phpcpd": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src",
            "src/String",
            "src/Collection",
            "tests"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}