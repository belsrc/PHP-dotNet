{
    "name": "belsrc/php-dotnet",
    "type": "library",
    "description": ".Net like methods and classes in PHP",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/belsrc/php-dotnet",
    "keywords": [".net", "collection", "list", "dictionary", "string"],
    "authors": [
        {
            "name": "Bryan Kizer",
            "email": "bryan@bryanckizer.com"
        }
    ],
    "support": {
      "issues": "https://github.com/belsrc/php-dotnet/issues"
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*@dev",
        "sami/sami": "1.4.*",
        "pdepend/pdepend" : "1.1.*",
        "sebastian/phpcpd": "2.0.*"
    },
    "autoload": {
        "files": ["src/Using.php"],
        "psr-4": {
            "Belsrc\\PhpDotNet\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
